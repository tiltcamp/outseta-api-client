{
  "name": "outseta-api-client",
  "version": "0.0.1",
  "description": "A lightweight API client for Outseta.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "npx eslint ./src ./test --ext .ts,.js --max-warnings 0",
    "build": "npx tsc --project tsconfig.lib.json",
    "build:docs": "npx typedoc src/index.ts",
    "test": "npx karma start karma.conf.js --single-run",
    "test:watch": "npx karma start karma.conf.js --auto-watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiltcamp/outseta-api-client.git"
  },
  "author": "TiltCamp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiltcamp/outseta-api-client/issues"
  },
  "homepage": "https://github.com/tiltcamp/outseta-api-client#readme",
  "devDependencies": {
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "jasmine": "^3.6.3",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-typescript": "^5.2.0",
    "pretender": "^3.4.3",
    "typedoc": "^0.20.16",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0"
  }
}
